<div class="container-fluid">
  <h2>Libros</h2>
  <br>

    <table class="table" id="tablaLibros">
      <tbody>

        <form method="GET">
          <div class="form-group has-search">
            <span class="fa fa-search form-control-feedback"></span>
            <input type="text" id="buscar_libro" name="buscarLibro" class="form-control" placeholder="Buscar Libro">
          </div>
        </form>
  </tbody>

  <thead>

    <th>Nombre</th>
    <th>Autor</th>
    <th>Stock</th>
    <th>Precio</th>
    <th>ISBN</th>
    <th colspan="2"></th>
  </thead>


  </table>

  <button onclick="libros()" id="btnCargar" class="btn btn-success">Cargar tabla</button>

  <ul class="pagination justify-content-center" style="margin:20px 0">
  </ul>


</div>